I"(function() {
  $(window).bind('beforeunload', function() {
    $.ajax({
      type: 'GET',
      url: window.location.href + 'grounds/' + $('.ground').attr('id') + '/leave',
      success: function() {
        return 'Bye';
      },
      error: function() {}
    });
  });

  $(function() {
    $('#enter-ground-form').submit(function(e) {
      e.preventDefault();
      window.location.href = window.location.href + "grounds/" + ($('#enter-ground-form #name').val()) + "/join";
    });
    return $('.copy-ground-name').click(function() {
      var $temp;
      $temp = $('<input>');
      $('body').append($temp);
      $temp.val($('#ground-name').text()).select();
      document.execCommand('copy');
      $temp.remove();
      return $(this).html('Copied');
    });
  });

}).call(this);
